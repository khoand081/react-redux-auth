{
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": [
        "@babel/transform-react-jsx",
        "@babel/plugin-proposal-class-properties",
        "@babel/transform-runtime",
        "react-hot-loader/babel",
        "babel-plugin-styled-components",
        [
            "module-resolver",
            {
                "resolveDirs": ["App/src"]
            }
        ],
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": "css"
            }
        ]
    ]
}
